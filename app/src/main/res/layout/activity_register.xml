<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="edu.gatech.cats.cats_2340.controllers.RegisterActivity">

    <TextView
        android:id="@+id/name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        tools:layout_editor_absoluteX="38dp"
        tools:layout_editor_absoluteY="75dp"
        android:layout_marginStart="53dp"
        android:layout_below="@+id/nameLabel"
        android:layout_alignParentStart="true"
        android:layout_marginTop="19dp" />

    <TextView
        android:id="@+id/userID"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        tools:layout_editor_absoluteX="38dp"
        tools:layout_editor_absoluteY="162dp"
        android:layout_marginTop="13dp"
        android:layout_below="@+id/userIDLabel"
        android:layout_alignStart="@+id/userIDLabel" />

    <TextView
        android:id="@+id/password"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        tools:layout_editor_absoluteX="38dp"
        tools:layout_editor_absoluteY="247dp"
        android:layout_marginTop="10dp"
        android:layout_below="@+id/passwordLabel"
        android:layout_alignStart="@+id/passwordLabel" />

    <TextView
        android:id="@+id/nameLabel"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Name"
        tools:layout_editor_absoluteX="38dp"
        tools:layout_editor_absoluteY="47dp"
        android:layout_alignParentTop="true"
        android:layout_alignStart="@+id/name"
        android:layout_marginTop="49dp" />

    <TextView
        android:id="@+id/userIDLabel"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="ID"
        tools:layout_editor_absoluteX="38dp"
        tools:layout_editor_absoluteY="132dp"
        android:layout_marginTop="26dp"
        android:layout_below="@+id/name"
        android:layout_alignStart="@+id/name" />

    <TextView
        android:id="@+id/passwordLabel"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Password"
        tools:layout_editor_absoluteX="38dp"
        tools:layout_editor_absoluteY="226dp"
        android:layout_below="@+id/userID"
        android:layout_alignStart="@+id/userID"
        android:layout_marginTop="27dp" />

    <TextView
        android:id="@+id/userTypeLabel"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="User Type"
        tools:layout_editor_absoluteX="38dp"
        tools:layout_editor_absoluteY="308dp"
        android:layout_below="@+id/password"
        android:layout_alignEnd="@+id/passwordLabel"
        android:layout_marginTop="24dp" />

    <Spinner
        android:id="@+id/userType"
        android:layout_width="278dp"
        android:layout_height="22dp"
        tools:layout_editor_absoluteX="38dp"
        tools:layout_editor_absoluteY="335dp"
        android:layout_marginTop="14dp"
        android:layout_below="@+id/userTypeLabel"
        android:layout_alignStart="@+id/password" />
</RelativeLayout>
