<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="edu.gatech.cats.cats_2340.controllers.GraphTimeRange">

    <Button
        android:id="@+id/cancel"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="onCancelPressed"
        android:text="Cancel"
        tools:layout_editor_absoluteX="47dp"
        tools:layout_editor_absoluteY="475dp" />

    <Button
        android:id="@+id/search"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="onSearchPressed"
        android:text="Search"
        tools:layout_editor_absoluteX="196dp"
        tools:layout_editor_absoluteY="475dp" />

    <TextView
        android:id="@+id/desc"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Start Date (YYYY-MM-DD)"
        tools:layout_editor_absoluteX="47dp"
        tools:layout_editor_absoluteY="70dp" />

    <TextView
        android:id="@+id/desc2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="End Date (YYYY-MM-DD)"
        tools:layout_editor_absoluteX="47dp"
        tools:layout_editor_absoluteY="248dp" />

    <EditText
        android:id="@+id/startDateText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:hint="Date"
        android:text="Date"
        tools:layout_editor_absoluteX="38dp"
        tools:layout_editor_absoluteY="328dp" />

    <EditText
        android:id="@+id/startDateText2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:hint="Date"
        android:text="Date"
        tools:layout_editor_absoluteX="47dp"
        tools:layout_editor_absoluteY="123dp" />

</RelativeLayout>
